  <script>
const CORRECT_PASSWORD = "7661220";

function checkPassword(){
  const input = document.getElementById("passwordInput");
  const errorMsg = document.getElementById("errorMsg");
  const passwordScreen = document.getElementById("passwordScreen");
  const animationContainer = document.getElementById("animationContainer");
  
  if(input.value === CORRECT_PASSWORD){
    errorMsg.textContent = "";
    passwordScreen.style.display = "none";
    animationContainer.style.display = "flex";
    startUnlock();
  } else {
    errorMsg.textContent = "‚ùå Wrong password!";
    input.value = "";
    startCrash(passwordScreen);
  }
}

function playRandomSound(){
  const audioContext = new (window.AudioContext || window.webkitAudioContext)();
  const freq = Math.random() * 2000 + 200;
  const type = ["sine", "square", "sawtooth"][Math.floor(Math.random() * 3)];
  
  const osc = audioContext.createOscillator();
  const gain = audioContext.createGain();
  
  osc.connect(gain);
  gain.connect(audioContext.destination);
  
  osc.type = type;
  osc.frequency.value = freq;
  gain.gain.setValueAtTime(0.1, audioContext.currentTime);
  gain.gain.exponentialRampToValueAtTime(0.01, audioContext.currentTime + 0.1);
  
  osc.start(audioContext.currentTime);
  osc.stop(audioContext.currentTime + 0.1);
}

function playAlarmSound(){
  const audioContext = new (window.AudioContext || window.webkitAudioContext)();
  
  const osc = audioContext.createOscillator();
  const gain = audioContext.createGain();
  
  osc.connect(gain);
  gain.connect(audioContext.destination);
  
  osc.type = "square";
  osc.frequency.setValueAtTime(800, audioContext.currentTime);
  osc.frequency.setValueAtTime(600, audioContext.currentTime + 0.1);
  
  gain.gain.setValueAtTime(0.4, audioContext.currentTime);
  gain.gain.exponentialRampToValueAtTime(0.35, audioContext.currentTime + 0.15);
  
  osc.start(audioContext.currentTime);
  osc.stop(audioContext.currentTime + 0.15);
}

function glitchElement(element){
  const x = (Math.random() - 0.5) * 100;
  const y = (Math.random() - 0.5) * 100;
  const rotate = (Math.random() - 0.5) * 360;
  const scaleX = 0.5 + Math.random() * 1.5;
  const scaleY = 0.5 + Math.random() * 1.5;
  element.style.transform = `translate(${x}px, ${y}px) rotate(${rotate}deg) skew(${(Math.random()-0.5)*20}deg) scaleX(${scaleX}) scaleY(${scaleY})`;
}

function glitchBackground(){
  const x = (Math.random() - 0.5) * 200;
  const y = (Math.random() - 0.5) * 200;
  const rotate = (Math.random() - 0.5) * 45;
  const scaleX = 0.8 + Math.random() * 0.8;
  const scaleY = 0.8 + Math.random() * 0.8;
  document.body.style.transform = `translate(${x}px, ${y}px) rotate(${rotate}deg) scaleX(${scaleX}) scaleY(${scaleY})`;
}

function startCrash(element){
  const crashDuration = 1500;
  const glitchInterval = setInterval(()=>{
    playAlarmSound();
    glitchElement(element);
    glitchBackground();
    
    // Random color flashes
    const colors = ["#ff0000", "#00ff00", "#0000ff", "#ffff00", "#ff00ff"];
    element.style.borderColor = colors[Math.floor(Math.random() * colors.length)];
    element.style.boxShadow = `0 0 ${Math.random() * 60}px ${colors[Math.floor(Math.random() * colors.length)]}`;
  }, 50);
  
  setTimeout(()=>{
    clearInterval(glitchInterval);
    // Final glitch burst before closing
    for(let i = 0; i < 15; i++){
      setTimeout(()=>{
        playAlarmSound();
        glitchElement(element);
        glitchBackground();
      }, i * 30);
    }
  }, crashDuration);
  
  setTimeout(() => {
    document.body.style.transform = "";
    window.close();
  }, crashDuration + 500);
}

function startUnlock(){
  const key = document.getElementById("key");
  const lock = document.getElementById("lock");
  const glow = document.getElementById("glow");
  const vaultButton = document.getElementById("vaultButton");
  
  glow.classList.add("pulse");
  
  // Initial unlock sequence - key goes in
  setTimeout(()=>{ key.style.left="25px"; },200);
  
  // Lock opens
  setTimeout(()=>{
    lock.textContent="üîì";
  },1400);
  
  // After key goes in, button appears and grows
  setTimeout(()=>{
    vaultButton.style.display = "block";
    startButtonRotation(vaultButton);
  }, 1600);
  
  // Lock falls and fades while button is growing
  setTimeout(()=>{
    lock.style.animation = "lockFall 0.8s ease-in forwards";
    glow.style.opacity = "0";
  }, 1800);
}

function startButtonRotation(button){
  let startTime = Date.now();
  let animationId;
  let isAnimating = true;
  
  function animate(){
    if(!isAnimating) return;
    
    const elapsed = (Date.now() - startTime) / 1000;
    const rotation = Math.sin(elapsed * 5) * 10;
    button.style.transform = `scale(1) rotate(${rotation}deg)`;
    animationId = requestAnimationFrame(animate);
  }
  
  animate();
  
  // Stop rotation when button is clicked
  button.addEventListener("click", ()=>{
    isAnimating = false;
    cancelAnimationFrame(animationId);
  }, { once: true });
}

