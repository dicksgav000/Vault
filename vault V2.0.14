  <style>
* { font-family: 'Outfit', sans-serif; }

body{
  margin:0;
  height:100vh;
  display:flex;
  justify-content:center;
  align-items:center;
  background:linear-gradient(135deg,#1a0a0a,#4a0e0e,#0d0505);
  overflow:hidden;
}

#passwordScreen{
  background:rgba(0,0,0,0.8);
  border:2px solid #dc2626;
  border-radius:16px;
  padding:40px;
  width:100%;
  max-width:400px;
  box-shadow:0 0 40px rgba(220,38,38,0.3);
  text-align:center;
  animation:slideIn 0.5s ease-out;
}

@keyframes slideIn{
  from{
    opacity:0;
    transform:translateY(-30px);
  }
  to{
    opacity:1;
    transform:translateY(0);
  }
}

#passwordTitle{
  color:#fff;
  font-size:28px;
  margin:0 0 30px 0;
  font-weight:700;
}

#passwordInput{
  width:100%;
  padding:12px 16px;
  font-size:16px;
  border:2px solid #444;
  border-radius:8px;
  background:rgba(255,255,255,0.05);
  color:#fff;
  margin-bottom:16px;
  transition:0.3s;
  box-sizing:border-box;
}

#passwordInput:focus{
  outline:none;
  border-color:#dc2626;
  background:rgba(255,255,255,0.1);
  box-shadow:0 0 20px rgba(220,38,38,0.4);
}

#passwordInput::placeholder{
  color:#888;
}

#unlockBtn{
  width:100%;
  padding:12px;
  font-size:16px;
  font-weight:600;
  cursor:pointer;
  border:none;
  border-radius:8px;
  background:#dc2626;
  color:white;
  transition:0.3s;
}
#unlockBtn:hover{
  background:#b91c1c;
  transform:scale(1.02);
}

#unlockBtn:active{
  transform:scale(0.98);
}

#errorMsg{
  color:#ff6b6b;
  font-size:14px;
  margin-top:12px;
  min-height:20px;
}

#animationContainer{
  position:absolute;
  display:none;
  flex-direction:column;
  align-items:center;
}

#vaultButton{
  position:absolute;
  padding:14px 32px;
  font-size:18px;
  font-weight:600;
  cursor:pointer;
  border:none;
  border-radius:8px;
  background:#dc2626;
  color:#000;
  z-index:10;
  opacity:1;
  transform:scale(0.05);
  display:none;
  box-shadow:0 0 20px rgba(220,38,38,0.6), 0 0 40px rgba(220,38,38,0.4), inset 0 0 20px rgba(255,255,255,0.3);
  animation:growButton 0.8s cubic-bezier(0.34, 1.56, 0.64, 1) forwards;
}

@keyframes growButton{
  0%{
    transform:scale(0.05);
    opacity:0;
  }
  to{
    transform:scale(1);
    opacity:1;
  }
}

@keyframes lockFall{
  0%{
    opacity:1;
    transform:translateY(0);
  }
  to{
    opacity:0;
    transform:translateY(200px);
  }
}

#vaultButton:hover{
  background:#dc2626;
  box-shadow:0 0 30px rgba(220,38,38,0.8), 0 0 60px rgba(220,38,38,0.6), inset 0 0 20px rgba(255,255,255,0.4);
}

#vaultButton:active{
  box-shadow:0 0 40px rgba(220,38,38,1), 0 0 80px rgba(220,38,38,0.8), inset 0 0 20px rgba(255,255,255,0.5);
  animation:boomEffect 0.6s ease-out forwards;
}

@keyframes rotateWave{
  0%{ transform:scale(0.05) rotate(0deg); }
  100%{ transform:scale(0.05) rotate(360deg); }
}

.fade-out-screen{
  animation:fadeOutAnim 0.8s ease-out forwards;
}

@keyframes fadeOutAnim{
  to{
    opacity:0;
    transform:scale(0.9);
  }
}

.lock-wrapper{ position:relative; }

.glow{
  position:absolute;
  width:140px;
  height:140px;
  border-radius:50%;
  background:radial-gradient(circle, rgba(255,0,0,0.6) 0%, rgba(255,0,0,0.2) 40%, transparent 70%);
  opacity:0;
}

.pulse{ animation:pulseAnim 1s infinite; }

@keyframes pulseAnim{
  0%{ transform:scale(0.8); opacity:0.3; }
  50%{ transform:scale(1.2); opacity:0.9; }
  100%{ transform:scale(0.8); opacity:0.3; }
}

.lock{ font-size:90px; position:relative; }
.key{
  font-size:60px;
  position:absolute;
  left:-160px;
  top:30px;
  transition:1.2s ease-in-out;
}

.shake{
  animation:tiltAnim 0.6s ease-in-out;
}
@keyframes tiltAnim{
  0%   { transform:rotate(0deg); }
  15%  { transform:rotate(-4deg); }
  30%  { transform:rotate(4deg); }
  45%  { transform:rotate(-3deg); }
  60%  { transform:rotate(3deg); }
  75%  { transform:rotate(-2deg); }
  90%  { transform:rotate(2deg); }
  100% { transform:rotate(0deg); }
}

.fade-in{
  opacity:0;
  transform:translateY(20px);
  animation:fadeUp 0.6s forwards;
}
@keyframes fadeUp{
  to{
    opacity:1;
    transform:translateY(0);
  }
}

.boom{
  animation:boomAnim 0.8s ease-out forwards;
  transform-origin: center center;
}
@keyframes boomAnim{
  0% { transform: scale(1); }
  15% { transform: scale(1.5); }
  100% { transform: scale(1); }
}

@keyframes boomEffect{
  0%{ transform:scale(1) rotate(0deg); }
  50%{ transform:scale(1.5) rotate(0deg); }
  100%{ transform:scale(1) rotate(0deg); }
}
